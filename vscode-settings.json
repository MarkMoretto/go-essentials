{
    "[go]": {
		"editor.insertSpaces": false,
		"editor.formatOnSave": false,
		"editor.codeActionsOnSave": {
			"source.organizeImports": false
		},
		"editor.suggest.snippetsPreventQuickSuggestions": false
    },
    "go.formatTool": "gofmt",
    "go.lintTool": "golangci-lint",
    "go.lintFlags": [
		"--fast"
    ]
}
